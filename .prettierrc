{
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "endOfLine": "auto",
    "plugins": [
        "prettier-plugin-organize-imports"
    ],
    "overrides": [
        {
          "files": ["**/*.json", "**/*.yml"],
          "options": {
                "tabWidth": 2
            }
        }
    ]
}
